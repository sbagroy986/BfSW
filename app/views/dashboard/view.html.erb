<h2>Here you can see everyone who has signed up </h2>
<p>You could even send them an email saying 'Hi'!</p>
<% @users.each do |user| %>
	<ul>
		<li><%= user.Name %><br>
		<%= user.Gender %><br>
		<%= user.Email %><br>
		<p>About <%= user.Name %> : <%= user.About %><br>
		<%= image_tag(user.image_url ) %></li>

	</ul>
<% end %>
<%= link_to 'Back', users_path %>

